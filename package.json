{
  "name": "bookmarkly",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch": "npm-watch",
    "go": "concurrently \"npm run watch\" \"npm run start\""
  },
  "watch": {
    "build": "src/"
  },
  "dependencies": {
    "@mantine/core": "^7.10.0",
    "@mantine/form": "^7.10.0",
    "@mantine/hooks": "^7.10.0",
    "@reduxjs/toolkit": "^2.2.5",
    "@tabler/icons-react": "^3.5.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "concurrently": "^8.2.2",
    "npm-watch": "^0.13.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "globals": {
      "chrome": true
    },
    "rules": {
      "no-undef": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
